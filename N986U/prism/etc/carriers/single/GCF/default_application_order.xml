<appOrder xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">

    <!-- folder : Google -->
    <folder 
        launcher:screen="0" 
        launcher:title="Google">        
        <!-- Drive -->
        <favorite 
            launcher:screen="0" 
            launcher:packageName="com.google.android.apps.docs" 
            launcher:className="com.google.android.apps.docs.app.NewMainProxyActivity" />
        <!-- Google Play Movies & TV -->
        <favorite 
            launcher:screen="1" 
            launcher:packageName="com.google.android.videos" 
            launcher:className="com.google.android.youtube.videos.EntryPoint" />
        <!-- Duo -->
        <favorite 
            launcher:screen="2" 
            launcher:packageName="com.google.android.apps.tachyon" 
            launcher:className="com.google.android.apps.tachyon.MainActivity" />
        <!-- Photos -->
        <favorite 
            launcher:screen="3" 
            launcher:packageName="com.google.android.apps.photos" 
            launcher:className="com.google.android.apps.photos.home.HomeActivity" />
    </folder>
    <!-- folder : Samsung -->
    <folder 
        launcher:screen="0" 
        launcher:title="Samsung">
        <!-- Galaxy Apps -->
        <favorite 
            launcher:screen="0" 
            launcher:packageName="com.sec.android.app.samsungapps" 
            launcher:className="com.sec.android.app.samsungapps.SamsungAppsMainActivity" />
        <!-- Samsung Internet -->
        <favorite 
            launcher:screen="1" 
            launcher:packageName="com.sec.android.app.sbrowser" 
            launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity" />
        <!-- My Files -->
        <favorite 
            launcher:screen="2" 
            launcher:packageName="com.sec.android.app.myfiles" 
            launcher:className="com.sec.android.app.myfiles.external.ui.MainActivity" />
        <!-- Samsung Gear -->
        <favorite 
            launcher:screen="3" 
            launcher:packageName="com.samsung.android.app.watchmanager" 
            launcher:className="com.samsung.android.app.watchmanager.setupwizard.SetupWizardWelcomeActivity" />
        <!-- Samsung Health -->
        <favorite 
            launcher:screen="4" 
            launcher:packageName="com.sec.android.app.shealth" 
            launcher:className="com.samsung.android.app.shealth.home.HomeMainActivity" />
        <!-- Samsung Notes -->
        <favorite 
            launcher:screen="5" 
            launcher:packageName="com.samsung.android.app.notes" 
            launcher:className="com.samsung.android.app.notes.memolist.MemoListActivity" />
        <!-- SmartThings -->
        <favorite 
            launcher:screen="6" 
            launcher:packageName="com.samsung.android.oneconnect" 
            launcher:className="com.samsung.android.oneconnect.ui.SCMainActivity" />
