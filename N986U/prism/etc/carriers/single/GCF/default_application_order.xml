        <favorite 
            launcher:screen="7" 
            launcher:packageName="com.samsung.knox.securefolder" 
            launcher:className="com.samsung.knox.securefolder.switcher.SecureFolderShortcutActivity" />
        <!-- Smart Switch -->
        <favorite 
            launcher:screen="8" 
            launcher:packageName="com.sec.android.easyMover" 
            launcher:className="com.sec.android.easyMover.DistributionActivity" /> 
