<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">

    <home>
        <!-- appwidget : Weather -->
        <appwidget 
            screen="0" 
            packageName="com.sec.android.daemonapp" 
            className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1" 
            x="0" 
            y="2" 
            spanX="4" 
            spanY="1" />

        <!-- appwidget : Search -->
        <appwidget 
            screen="0" 
            packageName="com.samsung.android.app.searchwidget" 
            className="com.samsung.android.app.searchwidget.ui.SearchWidget" 
            x="0" 
            y="3" 
            spanX="4" 
            spanY="1" />

        <!-- Galaxy Store -->
        <favorite 
            screen="0" 
            packageName="com.sec.android.app.samsungapps" 
            className="com.sec.android.app.samsungapps.SamsungAppsMainActivity" 
            x="0" 
            y="4" />

        <!-- Gallery -->
        <favorite 
            screen="0" 
            packageName="com.sec.android.gallery3d" 
            className="com.samsung.android.gallery.app.activity.GalleryActivity" 
            x="1" 
            y="4" />

        <!-- Galaxy Themes -->
        <favorite 
            screen="0" 
            packageName="com.samsung.android.themestore" 
            className="com.samsung.android.themestore.activity.Launcher" 
            x="2" 
            y="4" />

        <!-- Samsung Assistant -->
        <favorite 
            screen="0" 
            packageName="com.samsung.android.app.sreminder" 
            className="com.samsung.android.app.sreminder.phone.cardlist.SReminderActivity" 
            x="3" 
            y="4" />

        <!-- Clock -->
        <favorite 
            screen="1" 
            packageName="com.sec.android.app.clockpackage" 
            className="com.sec.android.app.clockpackage.ClockPackage" 
            x="0" 
            y="4" />

        <!-- Samsung Pay -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.spay" 
            className="com.samsung.android.spay.ui.SpayMainActivity" 
            x="1" 
            y="4" />

        <!-- Game Launcher -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.game.gamehome" 
            className="com.samsung.android.game.gamehome.activity.StartActivityCN" 
            x="2" 
            y="4" />

        <!-- folder : Microsoft 应用程序 -->
        <folder 
            screen="1" 
            title="Microsoft 应用程序" 
            postPosition="true" 
            x="3" 
            y="4">
            <!-- Microsoft Office -->
            <favorite 
                screen="0" 
                packageName="com.microsoft.office.officehub" 
                className="com.microsoft.office.officesuite.OfficeSuiteActivity" />
            <!-- LinkedIn -->
            <favorite 
                screen="1" 
                packageName="com.linkedin.android" 
                className="com.linkedin.android.authenticator.LaunchActivity" />
            <!-- Outlook -->
            <favorite 
                screen="2" 
                packageName="com.microsoft.office.outlook" 
                className="com.microsoft.office.outlook.MainActivity" />
        </folder>

    </home>



    <hotseat>
        <!-- Phone -->
        <favorite 
            screen="0" 
            packageName="com.samsung.android.dialer" 
            className="com.samsung.android.dialer.DialtactsActivity" />

        <!-- Messages -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.messaging" 
            className="com.android.mms.ui.ConversationComposer" />

        <!-- Samsung Internet -->
        <favorite 
            screen="2" 
            packageName="com.sec.android.app.sbrowser" 
            className="com.sec.android.app.sbrowser.SBrowserMainActivity" />

        <!-- Camera -->
        <favorite 
            screen="3" 
            packageName="com.sec.android.app.camera" 
            className="com.sec.android.app.camera.Camera" />

    </hotseat>

</favorites>